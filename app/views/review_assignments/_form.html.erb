<%= simple_form_for([@pull_request, @review_assignment]) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :team_membership, label_method: proc { |team_membership| member_name(team_membership) } %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
